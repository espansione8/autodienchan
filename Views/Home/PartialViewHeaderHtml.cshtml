    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <baseurl value='@Url.Content("~/")'></baseurl> 
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE" />
    <link href="~/Content/style/reponsive-layout.css" rel="stylesheet" />
    @Styles.Render("~/Content/components/icon")
    @Styles.Render("~/Content/components/css")
    @Styles.Render("~/Content/style/css")
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Content/library/jquery.blockUI.js"></script>
    <script src="~/Content/library/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/chosen/1.1.0/chosen.jquery.min.js"></script>
    <script src="~/Content/library/jquery.bootstrap-touchspin.js"></script>
    <script src="~/Content/library/ereward-jquery-migrate.min.js"></script>
    <script src="~/Content/library/ereward-modernizr.js"></script>
    <script src="~/Content/library/bootstrap-switch.js"></script>
    <script src="~/Content/library/target-admin.js"></script>
    <script src="~/Content/editer/editor.js"></script>
    <script src="~/Content/library/bootstrap-fileupload.js"></script>
    @Scripts.Render("~/bundles/kendo")
    @Scripts.Render("~/bundles/angularlib")
    <script src="~/Content/library/jquery/jquery.slimscroll.min.js"></script>
    <script src="~/Content/library/perfect-scrollbar.js"></script>   
    <script src="~/Content/assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/formValidation.min.js"></script>
    <script src="~/Scripts/bootstrapextend.min.js"></script>
    <script src="~/Scripts/lodash.min.js"></script>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/angularjs")
    <script src="~/Scripts/Paging/dirPagination.js"></script>
    <link href="/Content/components/library/bootstrap/css/e-reward-bootstrap.min.css" rel="stylesheet" />
    <link href="/Content/components/library/icons/fontawesome/assets/css/font-awesome.min.css" rel="stylesheet" />
    <script src="~/Content/Canvas/canvasLib.js"></script>
    <style>
        .modal-dialog {
            margin: 150px auto;
        }

        html.top-full .navbar.main .topnav > li.dropdown.dd-1 .dropdown-menu li, html.top-full .navbar.main .topnav > li.dropdown .dd-1 .dropdown-menu li {
            border-color: #e1e6ef;
            width: 249px;
        }

        html.top-full .navbar.main .topnav > li.dropdown .dropdown-menu li > a, html.top-full .navbar.main .topnav > li.dropdown .dropdown-menu li > a:hover, html.top-full .navbar.main .topnav > li.dropdown .dropdown-menu li > span {
            background: #363432 none repeat scroll 0 0;
            color: #c8c8c8;
            display: block;
            line-height: 36px;
            padding: 0 10px;
            width: 247px;
        }

        html.top-full .navbar.main .topnav > li.dropdown.dd-1 .dropdown-menu, html.top-full .navbar.main .topnav > li.dropdown .dd-1 .dropdown-menu {
            background: #fff none repeat scroll 0 0;
            border: 1px solid #c8c8c8;
            border-radius: 0;
            margin: 5px 0 0;
            width: 250px;
        }
    </style>

    <style>
        .icon-size {
            font-size: 17px;
        }

        #menuRight .flag-language {
            position: relative;
            float: left;
            height: 49px;
            line-height: 49px;
            display: block;
            padding: 0 0 0 10px !IMPORTANT;
        }

        #logoSp .img-responsive, .thumbnail > img, .thumbnail a > img, .carousel-inner > .item > img, .carousel-inner > .item > a > img {
            display: block;
            height: auto;
            max-width: 70% !important;
        }

        #logoSp .img-responsive, .thumbnail > img, .thumbnail a > img, .carousel-inner > .item > img, .carousel-inner > .item > a > img {
            display: block;
            height: auto;
            max-width: 55% !important;
        }

        #logoSp1 .img-responsive, .thumbnail > img, .thumbnail a > img, .carousel-inner > .item > img, .carousel-inner > .item > a > img {
            display: block;
            height: auto;
            max-width: 70% !important;
        }

        .box-img-logo .img-responsive, .thumbnail > img, .thumbnail a > img, .carousel-inner > .item > img, .carousel-inner > .item > a > img {
            display: block;
            height: auto;
            max-width: 70% !important;
        }

        html {
            overflow-x: hidden;
        }

        .help-block {
            color: red !important;
            display: block;
            margin-bottom: 10px;
            margin-top: 0;
        }
    </style>

</head>